def insertion_sort(arr):
    n = len(arr)

    for i in range(1, n):
        key = arr[i]          # current value to place
        j = i - 1

        while j >= 0 and arr[j] > key:
            arr[j + 1] = arr[j]   # shift bigger values right
            j -= 1

        arr[j + 1] = key     # insert in correct spot

    return arr


numbers = [8, 3, 5, 2, 9, 1]
print(insertion_sort(numbers))
