def maxx_sum_subarray(arr, w):
    n = len(arr)  
    maxx = 0  

    for i in range(0, n - w + 1 ):
        current = 0
        for j in range(i, i + w ):
            current = current + arr[j]
        maxx = max(current, maxx)
    
    return maxx


arr = [3, 8, 2, 5, 7, 6, 12] 
w = 4
print(maxx_sum_subarray(arr, w)) 
