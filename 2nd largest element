a = [64, 34, 25, 5, 22, 11, 90, 12]
print("sorted array", sorted(a))

n = len(a)

if a[0] > a[1]:
    max1 = a[0]
    max2 = a[1]
else:
    max2 = a[0]
    max1 = a[1]

for i in range (2, n):
    if a[i] > max1:
        max2 = max1
        max1 = a[i]

    elif  a[i]> max2:
        max2 = a[i]

print(max1)
print(max2)












